@article{mishraDesignApplicationKinetic2023,
  title = {Design and Application of a Kinetic Model of Lipid Metabolism in {{Saccharomyces}} Cerevisiae},
  author = {Mishra, Shekhar and Wang, Ziyu and Volk, Michael J. and Zhao, Huimin},
  year = {2023},
  month = jan,
  journal = {Metabolic Engineering},
  volume = {75},
  pages = {12--18},
  issn = {1096-7176},
  doi = {10.1016/j.ymben.2022.11.003},
  urldate = {2022-11-22},
  abstract = {Lipid biosynthesis plays a vital role in living cells and has been increasingly engineered to overproduce various lipid-based chemicals. However, owing to the tightly constrained and interconnected nature of lipid biosynthesis, both understanding and engineering of lipid metabolism remain challenging, even with the help of mathematical models. Here we report the development of a kinetic metabolic model of lipid metabolism in Saccharomyces cerevisiae that integrates fatty acid biosynthesis, glycerophospholipid metabolism, sphingolipid metabolism, storage lipids, lumped sterol synthesis, and the synthesis and transport of relevant target-chemicals, such as fatty acids and fatty alcohols. The model was trained on lipidomic data of a reference S. cerevisiae strain, single knockout mutants, and lipid overproduction strains reported in literature. The model was used to design mutants for fatty alcohol overproduction and the lipidomic analysis of the resultant mutant strains coupled with model-guided hypothesis led to discovery of a futile cycle in the triacylglycerol biosynthesis pathway. In addition, the model was used to explain successful and unsuccessful mutant designs in metabolic engineering literature. Thus, this kinetic model of lipid metabolism can not only enable the discovery of new phenomenon in lipid metabolism but also the engineering of mutant strains for overproduction of lipids.},
  langid = {english},
  keywords = {Fatty alcohol,Free fatty acid,Kinetic model,Lipid metabolism,PRIORITY-5},
  file = {/home/arin/Zotero/storage/QQ5YM2Z6/Mishra et al. - 2023 - Design and application of a kinetic model of lipid.pdf;/home/arin/Zotero/storage/KGQZYD6S/S1096717622001380.html}
}

@inproceedings{mitchellDeepLearningTechniques2023,
  title = {Deep {{Learning Techniques}} for {{Noise Annoyance Detection}}: {{Results}} from an Intensive Workshop at the {{Alan Turing Institute}}},
  booktitle = {Proceedings of {{Meetings}} on {{Acoustics}}},
  author = {Mitchell, Andrew and Brown, Emmeline and Deo, Ratneel and Hou, Yuanbo and {Kirton-Wingate}, Jasper and Liang, Jinhua and Sheinkman, Alisa and Soelistyo, Christopher and Sood, Hari and Wongprommoon, Arin and Xing, Kaiyue and Yip, Wingyan and Aletta, Francesco},
  year = {2023},
  month = may,
  address = {{Chicago, USA}},
  abstract = {Advancements in AI and ML have enabled us to combine automated sound source recognition and deep learning models for predicting subjective soundscape perception. We held a multidisciplinary, cross-institutional Data Study Group (DSG) to investigate how sound source information could be incorporated into deep learning models for predicting urban noise annoyance. We used a large-scale dataset of 2,980 15-sec recordings paired with 12,210 annoyance ratings (from 1-10) and sound source labels. A total of 14 neural networks and 4 conventional ML models were built. The best model was trained to simultaneously predict sound source labels and annoyance rating. It achieved an RMSE=1.07 for annoyance prediction and AUROC=0.88 for label classification, while a similarly structured model trained to predict annoyance ratings only (i.e. no sound source information) achieved RMSE=1.13. Results showed that including sound source labels as a simultaneous training output, rather than as an explicit model input resulted in the best performance. Overall, these models performed very well at predicting both annoyance ratings and identifying sources, providing a starting bed for automated annoyance detection systems. This presentation will provide context for the DSG and present conclusions drawn regarding approaches to applying deep learning techniques to noise annoyance detection.},
  copyright = {All rights reserved}
}

