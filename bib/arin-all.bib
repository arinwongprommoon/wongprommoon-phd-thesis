@article{elsemmanWholecellModelingYeast2022,
  title = {Whole-Cell Modeling in Yeast Predicts Compartment-Specific Proteome Constraints That Drive Metabolic Strategies},
  author = {Elsemman, Ibrahim E. and Rodriguez Prado, Angelica and Grigaitis, Pranas and Garcia Albornoz, Manuel and Harman, Victoria and Holman, Stephen W. and {van Heerden}, Johan and Bruggeman, Frank J. and Bisschops, Mark M. M. and Sonnenschein, Nikolaus and Hubbard, Simon and Beynon, Rob and {Daran-Lapujade}, Pascale and Nielsen, Jens and Teusink, Bas},
