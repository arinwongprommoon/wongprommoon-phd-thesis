@misc{cunyCellRegionFingerprints2021,
  title = {Cell Region Fingerprints Enable Highly Precise Single-Cell Tracking and Lineage Reconstruction},
  author = {Cuny, Andreas P. and Ponti, Aaron and K{\"u}ndig, Tomas and Rudolf, Fabian and Stelling, J{\"o}rg},
  year = {2021},
  month = oct,
