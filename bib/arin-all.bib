}

@article{montezanoFluxBalanceAnalysis2015,
  title = {Flux {{Balance Analysis}} with {{Objective Function Defined}} by {{Proteomics Data}}\textemdash{{Metabolism}} of {{Mycobacterium}} Tuberculosis {{Exposed}} to {{Mefloquine}}},
  author = {Montezano, Daniel and Meek, Laura and Gupta, Rashmi and Bermudez, Luiz E. and Bermudez, Jos{\'e} C. M.},
