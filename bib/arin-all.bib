@incollection{angChapterTenProbing2011,
  title = {Chapter {{Ten}} - {{Probing}} the {{Input}}\textendash{{Output Behavior}} of {{Biochemical}} and {{Genetic Systems}}: {{System Identification Methods}} from {{Control Theory}}},
  shorttitle = {Chapter {{Ten}} - {{Probing}} the {{Input}}\textendash{{Output Behavior}} of {{Biochemical}} and {{Genetic Systems}}},
  booktitle = {Methods in {{Enzymology}}},
  author = {Ang, Jordan and Ingalls, Brian and McMillen, David},
