@article{changEngineeringComplexCommunities2021,
  title = {Engineering Complex Communities by Directed Evolution},
  author = {Chang, Chang-Yu and Vila, Jean C. C. and Bender, Madeline and Li, Richard and Mankowski, Madeleine C. and Bassette, Molly and Borden, Julia and Golfier, Stefan and Sanchez, Paul Gerald L. and Waymack, Rachel and Zhu, Xinwen and {Diaz-Colunga}, Juan and Estrela, Sylvie and {Rebolleda-Gomez}, Maria and Sanchez, Alvaro},
  year = {2021},
  month = jul,
  journal = {Nature Ecology \& Evolution},
  volume = {5},
  number = {7},
  pages = {1011--1023},
  publisher = {{Nature Publishing Group}},
  issn = {2397-334X},
  doi = {10.1038/s41559-021-01457-5},
  urldate = {2022-04-09},
  abstract = {Directed evolution has been used for decades to engineer biological systems at or below the organismal level. Above the organismal level, a small number of studies have attempted to artificially select microbial ecosystems, with uneven and generally modest success. Our theoretical understanding of artificial ecosystem selection is limited, particularly for large assemblages of asexual organisms, and we know little about designing efficient methods to direct their evolution. Here, we have developed a flexible modelling framework that allows us to systematically probe any arbitrary selection strategy on any arbitrary set of communities and selected functions. By artificially selecting hundreds of in silico microbial metacommunities under identical conditions, we first show that the main breeding methods used to date, which do not necessarily let communities reach their ecological equilibrium, are outperformed by a simple screen of sufficiently mature communities. We then identify a range of alternative directed evolution strategies that, particularly when applied in combination, are well suited for the top-down engineering of large, diverse and stable microbial consortia. Our results emphasize that directed evolution allows an ecological structure\textendash function landscape to be navigated in search of dynamically stable and ecologically resilient communities with desired quantitative attributes.},
  copyright = {2021 The Author(s), under exclusive licence to Springer Nature Limited},
  langid = {english},
  keywords = {Community ecology,Evolutionary theory},
  file = {/home/arin/Zotero/storage/Y286R86M/Chang et al. - 2021 - Engineering complex communities by directed evolut.pdf;/home/arin/Zotero/storage/DMIVMEGV/s41559-021-01457-5.html}
}

@article{chanStandardizingBiomassReactions2017,
