@article{schonbornFlySilicoFluxBalance2019,
  title = {{{FlySilico}}: {{Flux}} Balance Modeling of {{Drosophila}} Larval Growth and Resource Allocation},
  shorttitle = {{{FlySilico}}},
  author = {Sch{\"o}nborn, J{\"u}rgen Wilhelm and Jehrke, Lisa and {Mettler-Altmann}, Tabea and Beller, Mathias},
