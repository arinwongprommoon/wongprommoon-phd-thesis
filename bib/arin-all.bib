@article{chenCharacterizingPhenotypicDiversity2022,
  title = {Characterizing Phenotypic Diversity of Trehalose Biosynthesis Mutants in Multiple Wild Strains of {{Saccharomyces}} Cerevisiae},
  author = {Chen, Anqi and Smith, Jeremy R and Tapia, Hugo and Gibney, Patrick A},
  year = {2022},
  month = aug,
  journal = {G3 Genes|Genomes|Genetics},
  pages = {jkac196},
  issn = {2160-1836},
  doi = {10.1093/g3journal/jkac196},
  urldate = {2022-08-09},
  abstract = {In the yeast Saccharomyces cerevisiae, trehalose-6-phospahte synthase (Tps1) and trehalose-6-phosphate phosphatase (Tps2) are the main proteins catalyzing intracellular trehalose production. In addition to Tps1 and Tps2, two putative regulatory proteins with less clearly defined roles also appear to be involved with trehalose production, Tps3 and Tsl1. While this pathway has been extensively studied in laboratory strains of S. cerevisiae, we sought to examine the phenotypic consequences of disrupting these genes in wild strains. Here we deleted the TPS1, TPS2, TPS3 and TSL1 genes in four wild strains and one laboratory strain for comparison. Although some tested phenotypes were not shared between all strains, deletion of TPS1 abolished intracellular trehalose, caused inability to grow on fermentable carbon sources and resulted in severe sporulation deficiency for all five strains. After examining tps1 mutant strains expressing catalytically inactive variants of Tps1, our results indicate that Tps1, independent of trehalose production, is a key component for yeast survival in response to heat stress, for regulating sporulation, and growth on fermentable sugars. All tps2{$\Delta$} mutants exhibited growth impairment on non-fermentable carbon sources, whereas variations were observed in trehalose synthesis, thermosensitivity and sporulation efficiency. tps3{$\Delta$} and tsl1{$\Delta$} mutants exhibited mild or no phenotypic disparity from their isogenic wild type although double mutants tps3{$\Delta$} tsl1{$\Delta$} decreased the amount of intracellular trehalose production in all five strains by 17\% - 45\%. Altogether, we evaluated, confirmed, and expanded the phenotypic characteristics associated trehalose biosynthesis mutants. We also identified natural phenotypic variants in multiple strains that could be used to genetically dissect the basis of these traits and then develop mechanistic models connecting trehalose metabolism to diverse cellular processes.},
  keywords = {PRIORITY-4},
  file = {/home/arin/Zotero/storage/3P5Z9NJG/Chen et al. - 2022 - Characterizing phenotypic diversity of trehalose b.pdf;/home/arin/Zotero/storage/AGWGL763/6656353.html}
}

@phdthesis{chenDissectingDynamicInteractions2022,
  type = {Thesis Fully Internal ({{DIV}})},
  title = {Dissecting Dynamic Interactions between Metabolic and Cell Cycle Oscillators: Development and Application of Novel Single-Cell Tools},
  shorttitle = {Dissecting Dynamic Interactions between Metabolic and Cell Cycle Oscillators},
  author = {Chen, Haoqi},
  year = {2022},
  address = {{[Groningen]}},
  doi = {10.33612/diss.207591836},
  abstract = {Cell cycle is a fundamental process of life where a cell replicates and partitions its biomass and genetic information. Various molecular activities oscillate during the cell cycle, with many of them controlled by an oscillating molecular circuit made of CDK/cyclins \textendash{} anaphase promoter complex. However, our group has recently shown in yeast cells that the concentration of some key metabolites oscillates autonomously from the CDK oscillator during the cell cycle. To understand how the metabolic oscillator could interact with the CDK oscillator and regulate cell cycle progression, this thesis applied existing as well as newly developed single-cell tools. In chapter 2, the researcher dynamically shifted nutrient supply for or depleted cell cycle related proteins in single yeast cells and validated a mathematical model where metabolic oscillator is separately coupled to early and late cell cycle components. In chapter 3, the researcher engineered a light-switchable version of a key metabolic enzyme by fusing it with a light-responding protein at computationally predicted sites, for controlling the metabolic oscillator. In Chapter 4, the researcher has probed the activity of a growth-regulating signalling pathway by measuring the localization of a downstream target of this pathway and the concentration of a signalling molecule using fluorescence microscopy. The pathway was found to be oscillating when cells are growing on acetate. Controlling signalling molecule production with a photo-switchable enzyme revealed the pathway's role in cell cycle initiation. Overall, this thesis provides tools and knowledge that will aid further understanding of cell cycle and metabolic oscillator interaction.},
  school = {University of Groningen},
  keywords = {PRIORITY-6}
}

