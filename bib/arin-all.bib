  langid = {english},
  file = {/home/arin/Zotero/storage/SDTGYIVA/Amponsah et al. - 2021 - Peroxiredoxins couple metabolism and cell division.pdf;/home/arin/Zotero/storage/GWJ8XEEQ/s41589-020-00728-9.html}
}

@article{anderlundExpressionEscherichiaColi1999,
  title = {Expression of the {{Escherichia}} Coli {{pntA andpntB Genes}}, {{Encoding Nicotinamide Nucleotide Transhydrogenase}}, in {{Saccharomyces}} Cerevisiae and {{Its Effect}} on {{Product Formation}} during {{Anaerobic Glucose Fermentation}}},
  author = {Anderlund, Mikael and Nissen, Torben L. and Nielsen, Jens and Villadsen, John and Rydstr{\"o}m, Jan and {Hahn-H{\"a}gerdal}, B{\"a}rbel and {Kielland-Brandt}, Morten C.},
