@article{leisePersistentCellAutonomousCircadian2012,
  title = {Persistent {{Cell-Autonomous Circadian Oscillations}} in {{Fibroblasts Revealed}} by {{Six-Week Single-Cell Imaging}} of {{PER2}}::{{LUC Bioluminescence}}},
  shorttitle = {Persistent {{Cell-Autonomous Circadian Oscillations}} in {{Fibroblasts Revealed}} by {{Six-Week Single-Cell Imaging}} of {{PER2}}},
