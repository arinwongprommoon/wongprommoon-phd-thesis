  langid = {english},
  keywords = {Applied microbiology,Biochemical networks,Computational models,Computer modelling},
  file = {/home/arin/Zotero/storage/2LA9RYD5/Elsemman et al. - 2022 - Whole-cell modeling in yeast predicts compartment-.pdf;/home/arin/Zotero/storage/XE3NI9TT/s41467-022-28467-6.html}
