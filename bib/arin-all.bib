@article{montano-gutierrezAnalysingMetaanalysingTimeseries2022,
  title = {Analysing and Meta-Analysing Time-Series Data of Microbial Growth and Gene Expression from Plate Readers},
  author = {{Monta{\~n}o-Gutierrez}, Luis Fernando and Moreno, Nahuel Manzanaro and Farquhar, Iseabail L. and Huo, Yu and Bandiera, Lucia and Swain, Peter S.},
  year = {2022},
