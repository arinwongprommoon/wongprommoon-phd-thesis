@article{hutchisonImprovedStatisticalMethods2015,
  title = {Improved {{Statistical Methods Enable Greater Sensitivity}} in {{Rhythm Detection}} for {{Genome-Wide Data}}},
  author = {Hutchison, Alan L. and {Maienschein-Cline}, Mark and Chiang, Andrew H. and Tabei, S. M. Ali and Gudjonson, Herman and Bahroos, Neil and Allada, Ravi and Dinner, Aaron R.},
  year = {2015},
