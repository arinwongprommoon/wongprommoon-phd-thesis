@article{yenushPpzProteinPhosphatases2002,
  title = {The {{Ppz}} Protein Phosphatases Are Key Regulators of {{K}}+ and {{pH}} Homeostasis: Implications for Salt Tolerance, Cell Wall Integrity and Cell Cycle Progression},
