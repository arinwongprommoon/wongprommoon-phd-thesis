@article{wickramaratneDisorderedAminoTerminus2022,
  title = {The {{Disordered Amino Terminus}} of the {{Circadian Enzyme Nocturnin Modulates Its NADP}}({{H}}) {{Phosphatase Activity}} by {{Changing Protein Dynamics}}},
  author = {Wickramaratne, Anushka C. and Li, Li and Hopkins, Jesse B. and Joachimiak, Lukasz A. and Green, Carla B.},
