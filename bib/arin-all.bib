@phdthesis{tripplerAutomaticIdentificationCell2020,
  title = {Automatic {{Identification}} of {{Cell Cycle Stages}} in {{Budding Yeast}}},
  author = {Trippler, Karen},
  year = {2020},
  abstract = {Due to the lack of alternatives, time-lapse single-cell microscopy relies upon phototoxic fluorescence markers, which potentially affect the function of the tagged protein, to monitor cellular processes, like the cell cycle. Here, a deep learning algorithm to identify cell cycle stages based on more readily available and less harmful bright-field images is presented. I first created an image dataset that is labelled via Free-Knot B-spline approximation of HTB2, a histone level marker, to distinguish four cell cycle stages in S. cerevisiae. The constructed dataset is validated by identifying biologically consistent cell cycle durations. It then serves as the ground truth dataset for the supervised training of a convolutional neural network. I demonstrate that it is possible to train a deep learning network to classify cell cycle stages from a bright-field image input by deliberately overfitting the neural network. However, an increased focus on the identification of individual cells and the exploration of transfer learning are required to improve generalisation and remove the need for fluorescence markers by deep learning in single-cell microscopy.},
  school = {University of Edinburgh}
}

