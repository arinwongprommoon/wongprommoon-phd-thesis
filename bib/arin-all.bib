@article{zhaoCriticalSlowingAttractive2020,
  title = {Critical Slowing down and Attractive Manifold: {{A}} Mechanism for Dynamic Robustness in the Yeast Cell-Cycle Process},
  shorttitle = {Critical Slowing down and Attractive Manifold},
  author = {Zhao, Yao and Wang, Dedi and Zhang, Zhiwen and Lu, Ying and Yang, Xiaojing and Ouyang, Qi and Tang, Chao and Li, Fangting},
  year = {2020},
  month = apr,
