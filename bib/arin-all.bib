  author = {Losa, Jos{\'e} and Leupold, Simeon and {Alonso-Martinez}, Diego and Vainikka, Petteri and Thallmair, Sebastian and Tych, Katarzyna M. and Marrink, Siewert J. and Heinemann, Matthias},
  year = {2022},
