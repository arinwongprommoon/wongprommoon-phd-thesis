  langid = {english},
  keywords = {Computational models,Genetic circuit engineering,Synthetic biology},
  file = {/home/arin/Zotero/storage/I6QTHZNU/Danino et al. - 2010 - A synchronized quorum of genetic clocks.pdf;/home/arin/Zotero/storage/LQKCCLMZ/nature08753.html}
}

