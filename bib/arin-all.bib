@article{kokinaPurineAuxotrophicStarvation2022,
  title = {Purine {{Auxotrophic Starvation Evokes Phenotype Similar}} to {{Stationary Phase Cells}} in {{Budding Yeast}}},
  author = {Kokina, Agnese and Tanilas, Kristel and Ozolina, Zane and Pleiko, Karlis and Shvirksts, Karlis and Vamza, Ilze and Liepins, Janis},
  year = {2022},
