@article{novakMitoticKinaseOscillation2022,
  title = {Mitotic Kinase Oscillation Governs the Latching of Cell Cycle Switches},
  author = {Novak, Bela and Tyson, John J.},
  year = {2022},
  month = may,
  journal = {Current Biology},
  issn = {0960-9822},
  doi = {10.1016/j.cub.2022.04.016},
  urldate = {2022-05-13},
  abstract = {In 1996, Kim Nasmyth1 proposed that the eukaryotic cell cycle is an alternating sequence of transitions from G1 to S-G2-M and back again. These two phases correlate to high activity of cyclin-dependent kinases (CDKs) that trigger S-G2-M events and CDK antagonists that stabilize G1 phase. We associated these ``alternative phases'' with the coexistence of two stable steady states of the biochemical reactions among CDKs and their antagonists.2,3 Transitions between these steady states (G1-to-S and M-to-G1) are driven by ``helper'' proteins. The fact that the transitions are irreversible is guaranteed by a ``latching'' property of the molecular switches, as we have argued in previous publications.4,5 Here, we show that if the latch is broken, then the biochemical reactions can swing back-and-forth across the transitions; either G1-S-G1-S \ldots{} (periodic DNA replication without mitosis or cell division) or M-(G1)-M-(G1) \ldots{} (periodic Cdc14 release, without fully exiting mitosis). Using mathematical modeling of the molecular control circuit in budding yeast, we provide a fresh account of aberrant cell cycles in mutant strains: endoreplication in the clb1-5{$\Delta$} strain6 and periodic release and resequestration of Cdc14 (an ``exit'' phosphatase) in the CLB2kd{$\Delta$} strain.7,8 In our opinion, these ``endocycles'' are not autonomous oscillatory modules that must be entrained by the CDK oscillator6,7 but rather inadvertent and deleterious oscillations that are normally suppressed by the CDK latching-gate mechanism.8},
  langid = {english},
  keywords = {bistability,Cdc14 endocycles,cell cycle,checkpoints,cyclin-dependent kinase,endoreplication cycles,mathematical model,PRIORITY-5},
  file = {/home/arin/Zotero/storage/ZXHPLKGC/Novak and Tyson - 2022 - Mitotic kinase oscillation governs the latching of.pdf;/home/arin/Zotero/storage/EKSZH69G/S0960982222005851.html}
}

