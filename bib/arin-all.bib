@article{qiaoNetworkDesignPrinciple2022,
  title = {Network Design Principle for Robust Oscillatory Behaviors with Respect to Biological Noise},
  author = {Qiao, Lingxia and Zhang, Zhi-Bo and Zhao, Wei and Wei, Ping and Zhang, Lei},
  editor = {Barkai, Naama},
  year = {2022},
