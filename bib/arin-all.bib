  keywords = {ATG mRNA,autophagy,Ccr4-Not complex,deadenylation,nitrogen starvation,Pop2/Caf1,post-transcriptional regulation,PRIORITY-4,RNA decay},
  file = {/home/arin/Zotero/storage/ZSRZ76TF/Yin et al. - 2022 - Bidirectional roles of the Ccr4-Not complex in reg.pdf;/home/arin/Zotero/storage/D2A5YIJU/15548627.2022.html}
